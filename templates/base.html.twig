        <meta name="keywords" content="location, vente, annonces, chambres, appartements, maisons, studio, terrains, gabon">
        <title>{% block title %}Location et vente immobilières{% endblock %}</title>
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;600;900&family=Poppins:wght@200&display=swap" rel="stylesheet">
        <link rel="shortcut icon" type="image/png" href="{{asset('assets/images/locagabs.png')}}"/>
        <link rel="stylesheet" href="{{asset('assets/css/home/home.css')}}">
        <link rel="stylesheet" href="{{asset('assets/css/home/gabonsvg.css')}}">
        <link rel="stylesheet" href="{{asset('assets/css/home/accordeon.css')}}">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css" integrity="sha512-MV7K8+y+gLIBoVD59lQIYicR65iaqukzvf/nwasF0nqhPay5w/9lJmVM2hMDcnK1OnMGCdVK+iQrJ7lzPJQd1w==" crossorigin="anonymous" referrerpolicy="no-referrer" />
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
