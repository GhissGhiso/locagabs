            <script src="{{asset('assets/js/home/selectprovil.js')}}" defer></script>
            <script src="{{asset('assets/js/home/accordeon.js')}}" defer></script>
            <script src="{{asset('assets/js/home/tabhome.js')}}" defer></script>
        {% endblock %}
    </head>
    <body>
        <div class="container">

            {% block header %}
                {% include "_partials/_header.html.twig" %}
            {% endblock %}
            
            {% include "_partials/_nav.html.twig" %}

            <main>
                {% block body %}{% endblock %}
            </main>
        </div>

        {% include "_partials/_footer.html.twig" %}
